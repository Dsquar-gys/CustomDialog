new DataGridTextColumn\n                {\n                    Header = \"Type\",\n                    Width = new DataGridLength(2d, DataGridLengthUnitType.Star),\n                    Binding = new Binding(\"Svm.Size\")\n                }\n            }\n        };\n\n        return grid;\n    }\n\n    public bool Match(object? data)\n    {\n        return data is BodyViewModel;\n    }\n}
